{
  "name": "wp-graphql-astro",
  "type": "module",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "dev": "astro dev",
    "start": "http-server ./dist",
    "build": "npm run build:branding && astro build && node fix-manifest.js && node src/scripts/generate-service-worker.js",
    "preview": "astro preview",
    "astro": "astro",
    "format": "prettier --write .",
    "lint:css": "stylelint \"src/**/*.{astro,css}\"",
    "lint:js": "eslint \"src/**/**.{astro,js,ts}\"",
    "lint": "npm run lint:css && npm run lint:js",
    "clean": "rm -rf dist",
    "prebuild": "npm run clean",
    "build:branding": "node src/scripts/process-templates.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "fix-manifest": "node fix-manifest.js",
    "generate-sw": "node src/scripts/generate-service-worker.js"
  },
  "dependencies": {
    "@astrojs/netlify": "^6.6.3",
    "@astrojs/react": "^4.4.2",
    "@astrojs/sitemap": "^3.6.0",
    "@fortawesome/fontawesome-svg-core": "^7.1.0",
    "@fortawesome/free-regular-svg-icons": "^7.1.0",
    "@fortawesome/free-solid-svg-icons": "^7.1.0",
    "@fortawesome/react-fontawesome": "^3.1.1",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "astro": "^5.16.4",
    "astro-seo": "^0.8.4",
    "darkmode-js": "^1.5.7",
    "dotenv": "^17.2.3",
    "http-server": "^14.1.1",
    "normalize.css": "^8.0.1",
    "react": "^19.2.1",
    "react-dom": "^19.2.1",
    "web-vitals": "^5.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.2.0",
    "@typescript-eslint/eslint-plugin": "^8.26.1",
    "@typescript-eslint/parser": "^8.26.1",
    "astro-eslint-parser": "^1.2.1",
    "autoprefixer": "^10.4.22",
    "cross-fetch": "^4.1.0",
    "cssnano": "^7.1.2",
    "eslint": "^9.39.1",
    "eslint-plugin-astro": "^1.5.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-react": "^7.37.4",
    "globals": "^16.5.0",
    "jsdom": "^27.2.0",
    "postcss": "^8.5.6",
    "postcss-cli": "^11.0.1",
    "postcss-html": "^1.8.0",
    "postcss-import": "^16.1.1",
    "postcss-preset-env": "^10.5.0",
    "prettier": "^3.7.4",
    "prettier-plugin-astro": "^0.14.1",
    "string-strip-html": "^13.5.0",
    "stylelint": "^16.26.1",
    "stylelint-config-standard": "^39.0.1",
    "typescript-eslint": "^8.48.1",
    "vitest": "^4.0.15"
  }
}
