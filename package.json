{
  "name": "wp-graphql-astro",
  "type": "module",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "dev": "astro dev",
    "start": "http-server ./dist",
    "build": "npm run build:branding && astro build && node fix-manifest.js && node src/scripts/generate-service-worker.js",
    "preview": "astro preview",
    "astro": "astro",
    "format": "prettier --write .",
    "lint:css": "stylelint \"src/**/*.{astro,css,scss}\"",
    "lint:js": "eslint \"src/**/**.{astro,js,ts}\"",
    "lint": "npm run lint:css && npm run lint:js",
    "clean": "rm -rf dist",
    "prebuild": "npm run clean",
    "build:branding": "node src/scripts/process-templates.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "fix-manifest": "node fix-manifest.js",
    "generate-sw": "node src/scripts/generate-service-worker.js"
  },
  "dependencies": {
    "@astrojs/netlify": "^6.2.3",
    "@astrojs/react": "^4.2.1",
    "@astrojs/sitemap": "^3.2.1",
    "@fortawesome/fontawesome-svg-core": "^6.7.2",
    "@fortawesome/free-regular-svg-icons": "^6.7.2",
    "@fortawesome/free-solid-svg-icons": "^6.7.2",
    "@fortawesome/react-fontawesome": "^0.2.2",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "astro": "^5.5.2",
    "astro-seo": "^0.8.4",
    "darkmode-js": "^1.5.7",
    "dotenv": "^16.4.7",
    "http-server": "^14.1.1",
    "normalize.css": "^8.0.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "string-strip-html": "^13.4.12",
    "web-vitals": "^4.2.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.22.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@typescript-eslint/eslint-plugin": "^8.26.1",
    "@typescript-eslint/parser": "^8.26.1",
    "astro-eslint-parser": "^1.2.1",
    "autoprefixer": "^10.4.21",
    "cross-fetch": "^4.1.0",
    "cssnano": "^7.0.6",
    "eslint": "^9.22.0",
    "eslint-plugin-astro": "^1.3.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-react": "^7.37.4",
    "globals": "^16.0.0",
    "jsdom": "^26.0.0",
    "postcss": "^8.5.3",
    "postcss-cli": "^11.0.1",
    "postcss-html": "^1.8.0",
    "postcss-import": "^16.1.0",
    "postcss-preset-env": "^10.1.5",
    "postcss-scss": "^4.0.9",
    "prettier": "^3.5.3",
    "prettier-plugin-astro": "^0.14.1",
    "sass": "^1.85.1",
    "stylelint": "^16.16.0",
    "stylelint-config-recommended-scss": "^14.1.0",
    "stylelint-config-standard": "^38.0.0",
    "typescript-eslint": "^8.26.1",
    "vitest": "^3.0.8"
  }
}
